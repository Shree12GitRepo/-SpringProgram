<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="COB" class="com.AccountInformation.SBI.CustomerOfBank">
		<property name="loan" value="Home Loan"/>
		<constructor-arg name="cusName" value="Sahoo Shree Raj Bijay"/>
		<constructor-arg>
			<bean class="com.AccountInformation.SBI.AccountType">
				<property name="creditAccount" value="778712345SBI"/>
				<property name="savingsAccount" value="100000345679SBI"/>
			</bean>
		</constructor-arg>
		
	</bean>

</beans>
